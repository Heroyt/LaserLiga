{
	"name": "heroyt/laserliga",
	"version": "1.2.3",
	"minimum-stability": "stable",
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.laserliga.cz"
		}
	],
	"scripts": {
		"install-app": "composer build && php install.php",
		"serve": "php -S localhost:8000 index.php",
		"test": "./vendor/bin/phpunit",
		"docs": "doxygen",
		"build-production": "composer update --no-dev && composer dump-autoload --no-dev && npm install && npm run build && npm prune --production",
		"build": "composer update && composer dump-autoload && npm install && npm run buildDev"
	},
	"require": {
		"tracy/tracy": ">2.8",
		"dibi/dibi": ">4.2",
		"latte/latte": ">2.10",
		"nette/utils": ">3.2",
		"psr/log": ">1.1",
		"nette/security": ">3.1",
		"ext-zip": "*",
		"php": "^8.1",
		"nette/http": ">3.1",
		"ext-gettext": "*",
		"nette/di": ">3.0",
		"ext-mbstring": "*",
		"ext-fileinfo": "*",
		"lsr/interfaces": "^0.2",
		"lsr/helpers": "^0.2",
		"lsr/core": "^0.2",
		"lsr/routing": "^0.2",
		"lsr/logging": "^0.2",
		"lsr/request": "^0.2",
		"lsr/auth": "^0.2",
		"ext-simplexml": "*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5",
		"gettext/gettext": "^5.7",
		"phpstan/phpstan": "^1.8",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan-dibi": "^1.0",
		"phpstan/phpstan-nette": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		},
		"files": [
			"include/functions.php"
		]
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	],
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
